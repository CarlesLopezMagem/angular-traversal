    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "jest": "^24.8.0",
    "jest-preset-angular": "^7.1.1",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
