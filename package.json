{
  "name": "angular-traversal-package",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build --prod traversal && cp ./README.md ./dist/traversal/",
    "build:demo": "ng build --prod --base-href /angular-traversal/dist/angular-traversal/",
    "test": "jest --no-cache",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand --ci",
    "lint": "ng lint",
    "get_version": "cat ./projects/traversal/package.json | grep version  | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]'"
  },
  "private": false,
  "dependencies": {
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "date-fns": "^2.9.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular-devkit/build-ng-packagr": "~0.803.25",
    "@angular/cli": "~8.3.25",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@ngrx/store-devtools": "^8.1.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "24.0.21",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "24.9.0",
    "jest-preset-angular": "8.0.0",
    "mrs-developer": "^1.1.3",
    "ng-packagr": "^5.1.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.38.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
