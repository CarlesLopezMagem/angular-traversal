{
  "name": "angular-traversal-package",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build --prod traversal && cp ./README.md ./dist/traversal/",
    "build:demo": "ng build --prod --base-href /angular-traversal/dist/angular-traversal/",
    "test": "jest --no-cache",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand --ci",
    "lint": "ng lint",
    "get_version": "cat ./projects/traversal/package.json | grep version  | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]'"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~9.0.0-rc.4",
    "@angular/common": "~9.0.0-rc.4",
    "@angular/compiler": "~9.0.0-rc.4",
    "@angular/core": "~9.0.0-rc.4",
    "@angular/forms": "~9.0.0-rc.4",
    "@angular/platform-browser": "~9.0.0-rc.4",
    "@angular/platform-browser-dynamic": "~9.0.0-rc.4",
    "@angular/router": "~9.0.0-rc.4",
    "core-js": "^2.5.4",
    "jest": "^24.8.0",
    "jest-preset-angular": "^7.1.1",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.0-rc.4",
    "@angular-devkit/build-ng-packagr": "~0.900.0-rc.4",
    "@angular/cli": "~9.0.0-rc.4",
    "@angular/compiler-cli": "~9.0.0-rc.4",
    "@angular/language-service": "~9.0.0-rc.4",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^24.0.14",
    "codelyzer": "^5.1.2",
    "ng-packagr": "^9.0.0-rc.2",
    "ts-node": "~7.0.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.6.4"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
